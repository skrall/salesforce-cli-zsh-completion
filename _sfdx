 {-a,--apiversion}'[API version number (44.0*,43.0)]:apiversion:_values apiversion "44.0" "43.0"' \
 --type'[type of the Lightning component (aura*,lwc)]:type:_values type "aura" "lwc"' \
 --json'[JSON output]' \
 --loglevel'[logging level for this command invocation (error*,trace,debug,info,warn,fatal)]:loglevel:_values loglevel "trace" "debug" "info" "warn" "error" "fatal"' \
 )
 ;;
 force:mdapi:convert)
 _command_args=(
 {-r,--rootdir}'[the root directory containing the Metadata API–formatted metadata]:rootdir:_path_files -/' \
 {-d,--outputdir}'[the output directory to store the source–formatted files]:outputdir:_path_files -/' \
 --json'[format output as json]' \
 --loglevel'[logging level for this command invocation (error*,trace,debug,info,warn,fatal)]:loglevel:_values loglevel "trace" "debug" "info" "warn" "error" "fatal"' \
 )
 ;;
 force:source:convert)
 _command_args=(
 {-r,--rootdir}'[a source directory other than the default package to convert]:rootdir:_path_files -/' \
 {-d,--outputdir}'[output directory to store the Metadata API–formatted files in]:outputdir:_path_files -/' \
 {-n,--packagename}'[name of the package to associate with the metadata-formatted files]:' \
 --json'[format output as json]' \
 --loglevel'[logging level for this command invocation (error*,trace,debug,info,warn,fatal)]:loglevel:_values loglevel "trace" "debug" "info" "warn" "error" "fatal"' \
 )
 ;;
 force:org:create)
 _command_args=(
 {-f,--definitionfile}'[path to a scratch org definition file]:file:_files' \
 {-j,--definitionjson}'[scratch org definition in json format ]:' \
 {-n,--nonamespace}'[creates the scratch org with no namespace]' \
 {-c,--noancestors}'[do not include second-generation package ancestors in the scratch org]' \
 {-i,--clientid}'[connected app consumer key]:' \
 {-s,--setdefaultusername}'[set the created org as the default username]' \
 {-a,--setalias}'[set an alias for for the created scratch org]:' \
 {-e,--env}'[environment where the scratch org is created: \[sandbox*,virtual,prototype\] (sandbox*,virtual,prototype)]:env:_values env "sandbox*" "virtual" "prototype"' \
 {-w,--wait}'[the streaming client socket timeout (in minutes) (default:6, min:2)]:' \
 {-d,--durationdays}'[duration of the scratch org (in days) (default:7, min:1, max:30)]:' \
 {-v,--targetdevhubusername}'[username or alias for the dev hub org; overrides default dev hub org]:' \
 --json'[format output as json]' \
 --loglevel'[logging level for this command invocation (error*,trace,debug,info,warn,fatal)]:loglevel:_values loglevel "trace" "debug" "info" "warn" "error" "fatal"' \
 )
 ;;
 force:package:create)
 _command_args=(
 {-n,--name}'[package name]:' \
 {-t,--packagetype}'[package type (Managed,Unlocked)]:packagetype:_values packagetype "Managed" "Unlocked"' \
 {-d,--description}'[package description]:' \
 {-e,--nonamespace}'[creates the package with no namespace; available only for unlocked packages.]' \
 {-r,--path}'[path to directory that contains the contents of the package]:path:_path_files -/' \
 {-v,--targetdevhubusername}'[username or alias for the dev hub org; overrides default dev hub org]:' \
 --json'[format output as json]' \
 --loglevel'[logging level for this command invocation (error*,trace,debug,info,warn,fatal)]:loglevel:_values loglevel "trace" "debug" "info" "warn" "error" "fatal"' \
 )
 ;;
